
A:	.WORD 123456
B:	.WORD 177450
ZERO=0.
NEG= -1.
POS= 1.;

START:	
	MOV #POS, R0
	MOV #ZERO, R1
	MOV #NEG, R2
	;TEST CC

	;REGISTER TO REGISTER
	MOV R2, R1

	; MEMORY TO REGISTER
	MOV A, SP

	; MEMORY TO MEMORY
	MOV A, B

	; REGISTER TO MEMORY
	MOV SP, B
	HALT
	.END START
