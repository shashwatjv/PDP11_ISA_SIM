
A:	.WORD 4
B:	.WORD 5
C:	.WORD 6

.BLKW 50
.BLKW 50
;	MOV PC,SP
;	TST -(SP)
;	DEC SP
;	MOV #START,SP
START:	
	MOV PC,SP
	TST -(SP)
	
	MOV #10,R1
	MOV A,-(SP)
	MOV B,-(SP)
	MOV C,-(SP)
	MOV #1,-(SP)
	JSR R1,SBR1
;	JSR PC,SBR1

	MOV (SP)+,R1
	MOV (SP)+,R2
	MOV (SP)+,R3
	BR DONE

SBR1:	MOV #7,R0
	MOV #2,R4
	MOV #3,R5
	RTS R1
;	RTS PC

DONE:	HALT
	
	.END START
	
	
 





